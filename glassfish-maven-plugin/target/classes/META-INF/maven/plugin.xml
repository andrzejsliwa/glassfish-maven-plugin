          <required>false</required>
          <editable>true</editable>
          <description>The user name of admin in Glassfish server</description>
        </parameter>
      </parameters>
      <configuration>
        <buildDir implementation="java.lang.String">${project.build.directory}</buildDir>
        <undeployName implementation="java.lang.String">${project.build.finalName}</undeployName>
        <glassfishHomeDir implementation="java.io.File">default</glassfishHomeDir>
        <glassfishHome implementation="java.lang.String">ENV</glassfishHome>
        <passwordfile implementation="java.lang.String">adminadmin</passwordfile>
        <fileName implementation="java.lang.String">${project.build.finalName}.${project.packaging}</fileName>
        <domain implementation="java.lang.String">domain1</domain>
        <user implementation="java.lang.String">admin</user>
