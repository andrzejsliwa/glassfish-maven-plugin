        </parameter>
        <parameter>
          <name>passwordfile</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The password of admin in Glassfish server</description>
        </parameter>
        <parameter>
          <name>undeployName</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>The name of the file or directory to deploy or undeploy.</description>
        </parameter>
        <parameter>
          <name>user</name>
