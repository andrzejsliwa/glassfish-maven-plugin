          <editable>true</editable>
          <description>The name of the file or directory to deploy or undeploy.</description>
        </parameter>
        <parameter>
          <name>domain</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The password of admin in Glassfish server</description>
        </parameter>
        <parameter>
          <name>fileName</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>The name of the file or directory to deploy or undeploy.</description>
        </parameter>
        <parameter>
          <name>glassfishHome</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>The location to Glassfish Home. This is a required configuration
parameter (unless GLASSFISH_HOME is set).</description>
